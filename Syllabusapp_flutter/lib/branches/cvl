class Cvl1sem1 extends StatefulWidget {
  @override
  Cvl1sem1State createState() => Cvl1sem1State();
}
class Cvl1sem1State extends State<Cvl1sem1> {
  SingleChildScrollView dataBody() {
    return SingleChildScrollView(
      scrollDirection: Axis.vertical,
      child: DataTable(
        columns: <DataColumn>[
          DataColumn(label: Text('Course.no'),
            numeric: false,
          ),
          DataColumn(label: Text('Subject name'),
            numeric: false,
          ),

        ],
        rows: cv1s1.map((name)=>DataRow(cells: [
          DataCell(
            Text(name.cno),
          ),
          DataCell(
            GestureDetector(
              child: Text(name.sname, style: TextStyle(decoration: TextDecoration.underline, color: Colors.brown)),
              onTap: () {
                if(name.sname=='Programming in C') {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => C()),
                  );
                }
                else if(name.sname=='Physics')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Phy()),
                  );
                }
                else if(name.sname=='Mathematics-1')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => M1()),
                  );
                }
                else if(name.sname=='English')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Eng()),
                  );
                }
                else if(name.sname=='Engineering Mechanics-I')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Em1()),
                  );
                }
                else if(name.sname=='Communication Skills-I')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Cs1()),
                  );
                }
                else if(name.sname=='Programming in C Lab')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Clab()),
                  );
                }
                else
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Phylab()),
                  );
                }
              },
            ),
          ),

        ],),).toList(),),);}
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        title: Text("cvl:year1:sem1"),
        backgroundColor: Colors.brown,
      ),
      body: Container(
        child: dataBody(),
      ),
    );
  }
}

class Cvl1sem2 extends StatefulWidget {
  @override
  Cvl1sem2State createState() => Cvl1sem2State();
}
class Cvl1sem2State extends State<Cvl1sem2> {
  SingleChildScrollView dataBody() {
    return SingleChildScrollView(
      scrollDirection: Axis.vertical,
      child: DataTable(
        columns: <DataColumn>[
          DataColumn(label: Text('Course.no'),
            numeric: false,
          ),
          DataColumn(label: Text('Subject name'),
            numeric: false,
          ),

        ],
        rows: cv1s2.map((name)=>DataRow(cells: [
          DataCell(
            Text(name.cno),
          ),
          DataCell(
            GestureDetector(
              child: Text(name.sname, style: TextStyle(decoration: TextDecoration.underline, color: Colors.brown)),
              onTap: () {
                if(name.sname=='Engineering Mechanics-II')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Em2()),
                  );
                }
                else if(name.sname=='Mathematics-2')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => M2()),
                  );
                }
                else if(name.sname=='Engineering Drawing')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Ed()),
                  );
                }
                else if(name.sname=='Chemistry')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Chem()),
                  );
                }
                else if(name.sname=='Scripting Languages')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Sl()),
                  );
                }
                else if(name.sname=='Communication Skills-II')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Cs2()),
                  );
                }
                else if(name.sname=='Engineering Workshop')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Ew()),
                  );
                }
                else
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Chemlab()),
                  );
                }
              },
            ),
          ),

        ],),).toList(),),);}
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        title: Text("cvl:year1:sem2"),
        backgroundColor: Colors.brown,
      ),
      body: Container(
        child: dataBody(),
      ),
    );
  }
}

class Cvl2sem1 extends StatefulWidget {
  @override
  Cvl2sem1State createState() => Cvl2sem1State();
}
class Cvl2sem1State extends State<Cvl2sem1> {
  SingleChildScrollView dataBody() {
    return SingleChildScrollView(
      scrollDirection: Axis.vertical,
      child: DataTable(
        columns: <DataColumn>[
          DataColumn(label: Text('Course.no'),
            numeric: false,
          ),
          DataColumn(label: Text('Subject name'),
            numeric: false,
          ),

        ],
        rows: cv2s1.map((name) =>
            DataRow(cells: [
              DataCell(
                Text(name.cno),
              ),
              DataCell(
                GestureDetector(
                  child: Text(name.sname, style: TextStyle(
                      decoration: TextDecoration.underline,
                      color: Colors.brown)),
                  onTap: () {
                    if (name.sname ==
                        'Building Materials and Concrete Technology') {
                      Navigator.push(
                        context,
                        MaterialPageRoute(builder: (context) => Bmct()),
                      );
                    }
                    else if (name.sname == 'Surveying') {
                      Navigator.push(
                        context,
                        MaterialPageRoute(builder: (context) => Sur()),
                      );
                    }
                    else if (name.sname == 'Mechanics of Materials') {
                      Navigator.push(
                        context,
                        MaterialPageRoute(builder: (context) => Mom()),
                      );
                    }
                    else if (name.sname == 'Mechanics of Fluids') {
                      Navigator.push(
                        context,
                        MaterialPageRoute(builder: (context) => Mf()),
                      );
                    }
                    else if (name.sname == 'Mathematics - III') {
                      Navigator.push(
                        context,
                        MaterialPageRoute(builder: (context) => M3()),
                      );
                    }
                    else if (name.sname == 'Communication Skills-I') {
                      Navigator.push(
                        context,
                        MaterialPageRoute(builder: (context) => Cs1()),
                      );
                    }
                    else if (name.sname == 'Surveying Lab') {
                      Navigator.push(
                        context,
                        MaterialPageRoute(builder: (context) => Surlab()),
                      );
                    }
                    else if (name.sname == 'Materials Testing Lab') {
                      Navigator.push(
                        context,
                        MaterialPageRoute(builder: (context) => Mtlab()),
                      );
                    }
                    else if (name.sname == 'Mechanics of Fluids Lab') {
                      Navigator.push(
                        context,
                        MaterialPageRoute(builder: (context) => Mflab()),
                      );
                    }
                    else {
                      Navigator.push(
                        context,
                        MaterialPageRoute(builder: (context) => Seminar1()),
                      );
                    }
                  },
                ),
              ),

            ],),).toList(),
      ),);
  }
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        title: Text("cvl:year2:sem1"),
        backgroundColor: Colors.brown,
      ),
      body: Container(
        child: dataBody(),
      ),
    );
  }
}

