                  );
                }
                else if(name.sname=='Personality Development-I')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Pd1()),
                  );
                }
                else if(name.sname=='Hydraulic Engineering Lab')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Helab()),
                  );
                }
                else if(name.sname=='Geo Science Lab')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Gslab()),
                  );
                }
                else if(name.sname=='Concrete Lab')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Conlab()),
                  );
                }
                else
                {
                  Navigator.push(
                    context,
