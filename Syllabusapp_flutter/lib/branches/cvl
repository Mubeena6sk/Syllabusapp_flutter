class Cvl4sem1 extends StatefulWidget {
  @override
  Cvl4sem1State createState() => Cvl4sem1State();
}
class Cvl4sem1State extends State<Cvl4sem1> {
  SingleChildScrollView dataBody() {
    return SingleChildScrollView(
      scrollDirection: Axis.vertical,
      child: DataTable(
        columns: <DataColumn>[
          DataColumn(label: Text('Course.no'),
            numeric: false,
          ),
          DataColumn(label: Text('Subject name'),
            numeric: false,
          ),

        ],
        rows: cv4s1.map((name)=>DataRow(cells: [
          DataCell(
            Text(name.cno),
          ),
          DataCell(
            GestureDetector(
              child: Text(name.sname, style: TextStyle(decoration: TextDecoration.underline, color: Colors.brown)),
              onTap: () {
                if(name.sname=='Managerial Economics and Financial Analysis')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Mefa()),
                  );
                }
                else if(name.sname=='Structural Design (Concrete)')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Sdc()),
                  );
                }
                else if(name.sname=='Advanced Water Resource Engineering')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => We()),
                  );
                }
                else if(name.sname=='Elective-I Remote Sensing and GIS')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Rsg()),
                  );
                }
                else
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => project()),
                  );
                }

              },
            ),
          ),

        ],),).toList(),),);}
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        title: Text("cse:year4:sem1"),
        backgroundColor: Colors.brown,
      ),
      body: Container(
        child: dataBody(),
      ),
    );
  }
}

class Cvl4sem2 extends StatefulWidget {
  @override
  Cvl4sem2State createState() => Cvl4sem2State();
}
class Cvl4sem2State extends State<Cvl4sem2> {
  SingleChildScrollView dataBody() {
    return SingleChildScrollView(
      scrollDirection: Axis.vertical,
      child: DataTable(
        columns: <DataColumn>[
          DataColumn(label: Text('Course.no'),
            numeric: false,
          ),
          DataColumn(label: Text('Subject name'),
            numeric: false,
          ),

        ],
        rows: cv4s2.map((name)=>DataRow(cells: [
          DataCell(
            Text(name.cno),
          ),
          DataCell(
            Text(name.sname),
          ),

        ],),).toList(),),);}
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        title: Text("Cvl:year4:sem2"),
        backgroundColor: Colors.brown,
      ),
      body: Container(
        child: dataBody(),
      ),
    );
  }
}
