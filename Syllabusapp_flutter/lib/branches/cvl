          DataCell(
            Text(name.cno),
          ),
          DataCell(
            GestureDetector(
              child: Text(name.sname, style: TextStyle(decoration: TextDecoration.underline, color: Colors.brown)),
              onTap: () {
                if(name.sname=='Design of Concrete Structures')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Dsc()),
                  );
                }
                else if(name.sname=='Environmental Science')
                {
                  Navigator.push(
                    context,
                    MaterialPageRoute(builder: (context) => Es()),
                  );
                }
                else if(name.sname=='Hydraulic Engineering')
