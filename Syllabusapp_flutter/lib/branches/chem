class Ch1s1 {
  String cno;
  String sname;
  String credits;

  Ch1s1({this.cno, this.sname, this.credits});
}
var ch1s1=<Ch1s1>[
  Ch1s1(cno:'-',sname: 'Free Elective-II',credits:'3'),
  Ch1s1(cno:'EC4000',sname: 'Comprehensive Viva',credits:'1'),
  Ch1s1(cno:'	EC4800',sname: 'Project',credits: '16'),

];
class Ch1s2 {
  String cno;
  String sname;
